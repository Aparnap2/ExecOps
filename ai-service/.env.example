# FounderOS AI Service Environment Configuration
# Copy this to .env and fill in your values

# PostgreSQL + pgvector connection
POSTGRES_CONNECTION_STRING=postgresql://founderos:founderos_secret@localhost:5432/founderos

# Neo4j connection (for Graphiti temporal memory)
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=founderos_secret

# Redis connection (for caching and Celery)
REDIS_URL=redis://localhost:6379

# OpenAI API key (for embeddings)
OPENAI_API_KEY=your-openai-api-key

# GitHub App credentials
GITHUB_APP_ID=your-github-app-id
GITHUB_PRIVATE_KEY=your-github-private-key
GITHUB_WEBHOOK_SECRET=your-webhook-secret

# Slack credentials
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_SIGNING_SECRET=your-slack-signing-secret

# LangFuse observability
LANGFUSE_PUBLIC_KEY=your-langfuse-public-key
LANGFUSE_SECRET_KEY=your-langfuse-secret-key
LANGFUSE_HOST=http://localhost:3000
