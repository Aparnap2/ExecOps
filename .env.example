# FounderOS Environment Configuration
# Copy this to .env and fill in values

# ====================
# AI Service (Python)
# ====================
OPENAI_API_KEY=sk-...
AI_SERVICE_HOST=0.0.0.0
AI_SERVICE_PORT=8000

# ====================
# Frontend (Next.js)
# ====================
NEXT_PUBLIC_API_URL=http://localhost:3000
AI_SERVICE_URL=http://localhost:8000

# ====================
# Database (PostgreSQL)
# ====================
DATABASE_URL=postgresql://founderos:founderos_secret@localhost:5432/founderos?schema=public

# ====================
# Redis
# ====================
REDIS_URL=redis://localhost:6379
